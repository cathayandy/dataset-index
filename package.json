{
  "name": "dataset-index",
  "version": "1.2.1",
  "description": "A dataset index page sample.",
  "main": "app.js",
  "scripts": {
    "dev": "node app.js",
    "serve": "nohup node app.js > dataset-index.log &",
    "start": "pm2 start app.js --name dataset-index -l dataset-index.log",
    "stop": "pm2 delete dataset-index",
    "restart": "pm2 restart dataset-index",
    "build": "$(npm bin)/webpack --progress --colors --watch",
    "dist": "NODE_ENV=production $(npm bin)/webpack --progress --colors",
    "config": "node makecfg.js"
  },
  "author": "cathayandy",
  "license": "MIT",
  "dependencies": {
    "antd": "^2.9.1",
    "isomorphic-fetch": "^2.2.1",
    "kcors": "^1.3.2",
    "koa": "^2.2.0",
    "koa-bodyparser": "^4.2.0",
    "koa-logger": "^2.0.1",
    "koa-mailer": "^0.1.0",
    "koa-static": "^3.0.0",
    "prompt": "^1.0.0",
    "react": "^15.5.3",
    "react-document-meta": "^2.1.1",
    "react-dom": "^15.5.3"
  },
  "devDependencies": {
    "babel-core": "^6.18.2",
    "babel-loader": "^6.2.8",
    "babel-plugin-import": "^1.1.0",
    "babel-plugin-transform-runtime": "^6.15.0",
    "babel-preset-es2015": "^6.18.0",
    "babel-preset-react": "^6.16.0",
    "babel-preset-stage-2": "^6.18.0",
    "css-loader": "^0.23.1",
    "extract-text-webpack-plugin": "^2.1.0",
    "file-loader": "^0.11.1",
    "less": "^2.7.2",
    "less-loader": "^4.0.3",
    "postcss-loader": "^1.3.3",
    "react-markdown-loader": "^1.1.10",
    "style-loader": "^0.13.1",
